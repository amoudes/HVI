clc; close all; clear all;
%%
material = {'A2','S2','T4','M4'};
t = [0.4, 0.8, 1.2];

d = 9.58;
td = t./d;

v = [5.5, 6.7, 7.5];

%%

for i = 1:length(material)
    for j = 1:length(v)
        for k = 1:length(td)
            name = [material{i},'_',num2str(td(j),3),'_',num2str(v(k))];
            fprintf('%s\n',name);

            if ~exist(name, 'dir')
                mkdir(name)
            end

        end
    end
end

